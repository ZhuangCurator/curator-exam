<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.curator.core.paper.mapper.TestPaperQuestionMapper">
    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.curator.core.paper.entity.TestPaperQuestion">
        <id column="test_paper_question_id" property="testPaperQuestionId" />
        <result column="test_paper_id" property="testPaperId" />
        <result column="exam_info_id" property="examInfoId" />
        <result column="question_id" property="questionId" />
        <result column="question_type" property="questionType" />
        <result column="question_sort" property="questionSort" />
        <result column="is_handled" property="handled" />
        <result column="user_answer" property="userAnswer" />
        <result column="user_point" property="userPoint" />
        <result column="is_deleted" property="deleted" />
        <result column="create_time" property="createTime" />
        <result column="update_time" property="updateTime" />
    </resultMap>

    <insert id="batchAddPaperQuestion">
        insert into test_paper_question(test_paper_question_id,test_paper_id,exam_info_id,
        question_id,question_type,question_sort,is_deleted,create_time,update_time)
        values
        <foreach collection="list" item="item" index="index" separator=",">
            (#{item.testPaperQuestionId}, #{item.testPaperId}, #{item.examInfoId}, #{item.questionId},
            #{item.questionType},#{item.questionSort}, #{item.deleted}, #{item.createTime}, #{item.updateTime})
        </foreach>
    </insert>

</mapper>
